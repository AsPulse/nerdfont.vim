Describe nerdfont#path
  Describe #find({path})
    It returns a jQuery glyph for 'xxxxxjquery.yyyyy.js'
      let glyph = nerdfont#path#find('xxxxxjquery.yyyyy.js')
      Assert Equals(glyph, '')
    End

    It returns a gulpfile glyph for 'gulpfile.js'
      let glyph = nerdfont#path#find('gulpfile.js')
      Assert Equals(glyph, '')
    End

    It returns a JS glyph for 'index.js'
      let glyph = nerdfont#path#find('index.js')
      Assert Equals(glyph, '')
    End

    It returns a JS glyph for 'xxxxxjquery.yyyyy.js' with disable_pattern
      let glyph = nerdfont#path#find('xxxxxjquery.yyyyy.js', {
            \ 'disable_pattern': v:true,
            \})
      Assert Equals(glyph, '')
    End

    It returns a JS glyph for 'gulpfile.js' with disable_basename
      let glyph = nerdfont#path#find('gulpfile.js', {
            \ 'disable_basename': v:true,
            \})
      Assert Equals(glyph, '')
    End

    It returns a default glyph for 'index.js' with disable_extension
      let glyph = nerdfont#path#find('index.js', {
            \ 'disable_extension': v:true,
            \})
      Assert Equals(glyph, '')
    End

    It returns a default glyph for 'hogehogefoobar'
      let glyph = nerdfont#path#find('hogehogefoobar')
      Assert Equals(glyph, '')
    End
  End
End
